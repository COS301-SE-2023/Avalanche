                "to": "Dim Registrar Key",
                "columns": ["Code","Name","Company Name","Trading Name"]
            },
            {
                "table": "Dim Sales Detail",
                "from": "Dim Sales Detail Key",
                "to": "Dim Sales Detail Key",
                "columns": [ "System","Type","Source"]
            },
            {
